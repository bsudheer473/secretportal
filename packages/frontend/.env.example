# Cognito User Pool Client ID (from CDK output)
VITE_USER_POOL_CLIENT_ID=XXXXXXXXXXXXXXXXXXXXXXXXXX
VITE_COGNITO_CLIENT_ID=XXXXXXXXXXXXXXXXXXXXXXXXXX

# AWS Region
VITE_AWS_REGION=us-east-1

